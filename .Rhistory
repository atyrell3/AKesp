label_facets <- function(p,
open = "(",
close = ")",
tag_pool = letters,
x = -Inf,
y = Inf,
hjust = -0.5,
vjust = 1.5,
fontface = 2,
family = "",
...) {
gb <- ggplot2::ggplot_build(p)
lay <- gb$layout$layout
tags <- cbind(lay,
label = paste0(
open,
tag_pool[lay$PANEL],
close
),
x = x,
y = y
)
p + ggplot2::geom_text(
data = tags,
ggplot2::aes_string(
x = "x",
y = "y",
label = "label"
),
...,
hjust = hjust,
vjust = vjust,
fontface = fontface,
family = family,
inherit.aes = FALSE
)
}
